    <script>
      var prevScrollpos = window.pageYOffset;
      window.onscroll = function() {
        var currentScrollPos = window.pageYOffset;
        if (prevScrollpos > currentScrollPos) {
          document.getElementById("navline").style.top = "25px";
        } else {
          document.getElementById("navline").style.top = "-10rem";
        }
        prevScrollpos = currentScrollPos;
      }
      </script>
      <script>
        const navigation = document.getElementById("nav");
        const menu = document.getElementById("menu");
    
        menu.addEventListener("click", () => {
          // The navigation.children.length means the following :-
          // The children inside a parent are basically an array of elements; So, here I'm finding the length of the array aka how many children are inside the nav bar.
          //   Yup That's all.
          navigation.style.setProperty("--childenNumber", navigation.children.length);
    
          //    Casually Toggling Classes to make them animate on click
          //   Regular stuff ;)
          navigation.classList.toggle("active");
          menu.classList.toggle("active");
        });
      </script>
      <script>
        // Wait for the document to be fully loaded
        document.addEventListener('DOMContentLoaded', function () {
            // Find the WhatsApp icon element
            var whatsappIcon = document.querySelector('.whatsapp');
    
            // Add a click event listener to the icon
            whatsappIcon.addEventListener('click', function (event) {
                // Prevent the default link behavior
                event.preventDefault();
    
                // Define the phone number you want to open the chat with
                var phoneNumber = '9690901694'; // Replace with the actual phone number
    
                // Construct the WhatsApp link
                var whatsappLink = 'https://wa.me/' + phoneNumber;
    
                // Open the link in a new tab/window
                window.open(whatsappLink, '_blank');
            });
        });
    </script>
