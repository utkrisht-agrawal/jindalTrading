  <script>
    // Get references to modal buttons and modals
    const modalButtons = document.querySelectorAll('.btn-arrow');
    const modals = document.querySelectorAll('.modal');

    // Attach click event listeners to each modal button
    modalButtons.forEach((button, index) => {
      button.addEventListener('click', () => {
        // Open the corresponding modal
        modals[index].style.display = 'block';
        document.body.classList.add('modal-open');
      });
    });

    // Close the modals and enable scrolling when the close buttons are clicked
    const closeModalButtons = document.querySelectorAll('.close');
    closeModalButtons.forEach((closeButton, index) => {
      closeButton.addEventListener('click', () => {
        modals[index].style.display = 'none';
        document.body.classList.remove('modal-open');
      });
    });

    // Close the modals and enable scrolling when the user clicks outside the modals
    window.addEventListener('click', (event) => {
      modals.forEach((modal) => {
        if (event.target === modal) {
          modal.style.display = 'none';
          document.body.classList.remove('modal-open');
        }
      });
    });



  </script>

