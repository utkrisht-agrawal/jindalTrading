<script>
    function edit(value)
    {
        console.log(value);
        var etransportName = document.getElementById("etransportName").value;
        var etransportArea = document.getElementById("etransportArea").value;
        var etransportStation = document.getElementById("etransportStation").value;
        var etransportOwner = document.getElementById("etransportOwner").value;
        var ebankName = document.getElementById("ebankName").value;
        var ebankAccountNumber = document.getElementById("ebankAccountNumber").value;
        var eifscCode = document.getElementById("eifscCode").value;

		$.post("/transportDetails",
		{
			id: value,
			transportName: etransportName,
			transportArea: etransportArea,
			transportStation: etransportStation,
			transportOwner: etransportOwner,
			bankName: ebankName,
			bankAccountNumber: ebankAccountNumber,
			ifscCode: eifscCode,
			op:"edt"
		},
		function (data, status) {
			console.log(data);
			console.log(status);
			setInterval('refreshPage()', 500);
		}
		);
    }
</script>

